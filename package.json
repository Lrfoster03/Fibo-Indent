{
  "name": "fibo-indent",
  "displayName": "Fibo-Indent",
  "description": "Ensure your indentations are up to fibonacci standards",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fibonacci-indent.reindent",
        "title": "Fibo-Format"
      },
      {
        "command": "fibonacci-indent.tab",
        "title": "Fibonacci Indent Tab"
      },
      {
        "command": "fibonacci-indent.shiftTab",
        "title": "Fibonacci Indent Shift Tab"
      }
    ],
    "keybindings": [
      {
        "command": "fibonacci-indent.tab",
        "key": "tab",
        "when": "editorTextFocus"
      },
      {
        "command": "fibonacci-indent.shiftTab",
        "key": "shift+tab",
        "when": "editorTextFocus"
      }
    ]
  },
  "activationEvents": [],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
